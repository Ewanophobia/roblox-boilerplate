{
    "name": "roblox-template",
    "emitLegacyScripts": false,
    "tree": {
        "$className": "DataModel",
        "ReplicatedStorage": {
            "Packages": {
                "$path": "Packages"
            },
            "Common": {
                "$path": "src/Common"
            },
            "Assets": {
                "$path": "assets"
            },
            "Library": {
                "$path": "lib"
            },
            "Client": {
                "$path": "src/Client"
            }
        },
        "ServerScriptService": {
            "Server": {
                "$path": "src/Server"
            }
        }
    }
}